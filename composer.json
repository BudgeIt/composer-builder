{
    "name": "budgeit/composer-builder",
    "description": "A Composer plugin to install external dependencies and run build commands",
    "license": "MIT",
    "type": "composer-plugin",
    "require": {
        "php": ">=5.5",
        "composer-plugin-api": "^1.0",
        "symfony/process": "^2.7"
    },
    "require-dev": {
        "composer/composer": "dev-master"
    },
    "autoload": {
        "psr-4": {"BudgeIt\\ComposerBuilder\\": "src/"}
    },
    "extra": {
        "class": "BudgeIt\\ComposerBuilder\\Plugin"
    }
}
